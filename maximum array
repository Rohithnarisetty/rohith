#include <stdio.h>

int main() {
    int array[10] = {10, 20, 30, 40, 50, 60, 70, 80, 90, 1000};  // array of 10 integers
    int max = array[0];  // initialize max to the first element of the array

    // loop through the array and compare each element to the current max
for (int i = 1; i < 10; i++) {
        if (array[i] > max) {
            max = array[i];  // update max if the current element is greater
        }
    }

    printf("The maximum number in the array is: %d", max);

    return 0;
}
